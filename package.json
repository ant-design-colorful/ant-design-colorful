{
  "private": false,
  "name": "antd-colorful",
  "version": "0.0.2",
  "scripts": {
    "start": "dumi dev",
    "build": "lessc index.less index.css -x",
    "docs:build": "dumi build",
    "docs-dev:build": "UMI_ENV=dev dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "deploy": "npm run docs:build && npm run docs:deploy",
    "release": "npm run build && npm publish",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "lint:style": "stylelint packages/**/*.less",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "main": "index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/ant-design-colorful/ant-design-colorful",
    "branch": "main"
  },
  "author": "ant-design-colorful",
  "homepage": "https://ant-design-colorful.github.io/ant-design-colorful/",
  "keywords": [
    "antd",
    "ant",
    "design",
    "components",
    "react",
    "ui"
  ],
  "files": [
    "index.css",
    "README.md",
    "CHANGELOG.md"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ant-design-colorful/ant-design-colorful/issues"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "antd": "^4.14.0",
    "react": "^17.0.1",
    "react-ripples": "^2.2.1"
  },
  "devDependencies": {
    "@umijs/test": "^3.0.5",
    "dumi": "^1.1.9",
    "father-build": "^1.17.2",
    "gh-pages": "^3.0.0",
    "less": "^4.0.0",
    "lint-staged": "^10.0.7",
    "prettier": "^2.1.2",
    "stylelint": "^13.0.0",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^21.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.1.0",
    "stylelint-order": "^4.0.0",
    "umi-plugin-gh-pages": "0.x",
    "yorkie": "^2.0.0"
  }
}
